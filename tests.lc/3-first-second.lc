;; -*- mode: scheme -*-

[(lambda (select-first)

   ((select-first <ARG1>) <ARG2>))

 (lambda (select-first:1st)
   (lambda (select-first:2nd)
     select-first:1st))]



[(lambda (select-second)

   ((select-second <ARG1>) <ARG2>))

 (lambda (select-second:1st)
   (lambda (select-second:2nd)
     select-second:2nd))]
